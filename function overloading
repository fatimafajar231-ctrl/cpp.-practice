#include<iostream>
using namespace std;

//funtion overloading means defining multiple function with the same name but with different parameter(type,numer)

int add(int a, int b)
{
    return a+b;
}

double add(double a, double b)
{
    return a+b;
}

int main()
{
    cout<<add(5,3);
    cout<<add(2.5,4.3);
    return 0;
}