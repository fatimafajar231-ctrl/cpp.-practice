#include<iostream>
using namespace std;
int main()
{
    //syntax
    //initialization
    //while(condition){
    //code to repeat
    //}

    int i=1;
    while(i<=5)
    {
        cout<<i<<""  ;
        i++;
    }

    int pin;
    cout << "Enter PIN: "; // Initial prompt
    cin >> pin;

    while (pin != 9999)
    {
        cout << "Incorrect PIN" << endl;
        cout << "Enter PIN again: ";
        cin >> pin;
    }
    cout << "Correct PIN" << endl;
    
    return 0;

}