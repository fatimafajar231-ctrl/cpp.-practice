class Student{
    private:
    int roll;
    sting name;
    public:
    Student(int r, sring n){
        roll = r;
        name = n;
    }
    Student(const Student &s){
        roll = s.roll;
        name = " ";
    }
};
int main(){
    Student s1(101, "Amit");
    Student s2 = s1;
}